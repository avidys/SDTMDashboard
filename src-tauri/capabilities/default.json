{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "store:default",
    "core:default",
    "dialog:allow-open",
    "dialog:default"
  ],
  "allowlist": {
    "fs": { "readTextFile": true, "scope": ["$DOCUMENT/*", "$DESKTOP/*"] },
    "dialog": { "open": true }
  }
}
